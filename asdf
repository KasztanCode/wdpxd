#include <iostream>
#include <vector>
using namespace std;
int sumaKutasa(vector<int>c){
    int wynik = 0;
    for (int i = 0; i < c.size(); ++i) {
        wynik +=c[i];
    }
    return wynik;
}

vector<int> kutasKozla(int x){
    vector<int>pizda;
    for (int i = 1; i <= x; ++i) {
        pizda.push_back(i*6);
    }
    return pizda;
}

int main() {
    cout<<sumaKutasa(kutasKozla(4));
    return 0;

}

